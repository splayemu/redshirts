{
  "name": "redshirts-prototype",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "move-phaser": "cp ./node_modules/phaser/build/phaser.js ./game/assets/js/lib/phaser.js",
    "move-easystar": "cp ./node_modules/easystarjs/bin/*min.js ./game/assets/js/lib/easystar.min.js",
    "move-d3": "cp ./node_modules/d3-**/build/*min.js ./game/assets/js/lib/",
    "move-all": "npm run move-phaser && npm run move-easystar && npm run move-d3",
    "start-dev": "npm run move-all && browser-sync start -s './game' -f './game'",
    "start": "npm run move-all && http-server ./game"
  },
  "author": "Spencer Apple (http://spencerapple.com/)",
  "license": "MIT",
  "dependencies": {
    "d3-color": "^1.0.2",
    "d3-interpolate": "^1.1.3",
    "d3-scale-chromatic": "^1.1.1",
    "easystarjs": "^0.3.1",
    "http-server": "^0.9.0",
    "phaser": "^2.6.2"
  },
  "devDependencies": {
    "browser-sync": "^2.11.1"
  }
}
